  "main": "index.js",
  "scripts": {
    "buildCompositionLogic": "PARADIGM=composition webpack --config webpack.config.logic.js",
    "watchCompositionLogic": "PARADIGM=composition webpack-dev-server --mode development --watch --config webpack.config.logic.js",
    "buildInheritanceLogic": "PARADIGM=inheritance webpack --config webpack.config.logic.js",
    "watchInheritanceLogic": "PARADIGM=inheritance webpack-dev-server --mode development --watch --config webpack.config.logic.js",
    "copySVG": "mkdir -p distUI && mkdir -p distUI/assets && cp -R ./ui_exercise/assets/*.svg ./distUI/assets",
    "buildUI": "npm run copySVG && webpack --config webpack.config.ui.js",
    "watchUI": "npm run copySVG && webpack-dev-server --mode development --watch --config webpack.config.ui.js"
  },
